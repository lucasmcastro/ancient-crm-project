<h1>Editing contact_form</h1>

<% form_for(@contact_form) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :contactable %><br />
    <%= f.text_field :contactable %>
  </p>
  <p>
    <%= f.label :contact_type %><br />
    <%= f.text_field :contact_type %>
  </p>
  <p>
    <%= f.label :contact %><br />
    <%= f.text_field :contact %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @contact_form %> |
<%= link_to 'Back', contact_forms_path %>