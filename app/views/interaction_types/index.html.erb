<h1>Listing interaction_types</h1>

<table>
  <tr>
    <th>Creator</th>
    <th>Name</th>
  </tr>

<% @interaction_types.each do |interaction_type| %>
  <tr>
    <td><%=h interaction_type.creator %></td>
    <td><%=h interaction_type.name %></td>
    <td><%= link_to 'Show', interaction_type %></td>
    <td><%= link_to 'Edit', edit_interaction_type_path(interaction_type) %></td>
    <td><%= link_to 'Destroy', interaction_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New interaction_type', new_interaction_type_path %>